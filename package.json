{
  "name": "itron",
  "version": "1.0.4",
  "bin": "dist/bin/itron.js",
  "description": "Help build applications using electron and webpack",
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "dev": "rimraf dist && babel src -w -d dist -s inline -D --include-dotfiles",
    "build": "rimraf dist && babel src -d dist -s --no-comments -D --include-dotfiles"
  },
  "dependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/runtime": "^7.13.9",
    "@craco/craco": "^6.1.1",
    "@ices/locale-webpack-plugin": "^1.4.1",
    "@ices/theme-webpack-plugin": "^1.4.1",
    "@ices/use-loader": "^1.4.0",
    "babel-jest": "25.5.1",
    "babel-plugin-react-css-modules": "^5.2.6",
    "bindings": "^1.5.0",
    "chalk": "3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "commander": "6.2.1",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.1 <=4.3.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "electron-builder": "^22.10.5",
    "electron-notarize": "^1.0.0",
    "electron-rebuild": "1.11.0",
    "file-loader": "4.3.0",
    "find-up": "4.1.0",
    "fs-extra": "8.1.0",
    "js-yaml": "^4.0.0",
    "json5": "^2.2.0",
    "loader-utils": "^2.0.0",
    "node-fetch": "^2.6.1",
    "portfinder": "^1.0.28",
    "prettier": "1.19.1",
    "sass": "^1.32.8",
    "schema-utils": "2.7.1",
    "slice-ansi": "3.0.0",
    "strip-ansi": "^6.0.0",
    "term-size": "^2.2.1",
    "ts-loader": "7.0.0",
    "unzip-crx-3": "^0.2.0",
    "wait-on": "^5.2.1",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-merge": "^5.7.3",
    "widest-line": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@types/node": "^14.14.32",
    "electron": "^12.0.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "@ices/react-locale": "^1.4.1",
    "@ices/theme": "^1.2.4",
    "babel-loader": "^7.0.0",
    "electron": ">=8",
    "webpack": ">=4"
  },
  "peerDependenciesMeta": {
    "@ices/react-locale": {
      "optional": true
    },
    "@ices/theme": {
      "optional": true
    },
    "babel-loader": {
      "optional": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json,md,yml}": [
      "prettier --write"
    ]
  },
  "repository": {
    "url": "https://github.com/icesjs/itron"
  },
  "author": {
    "name": "Stone",
    "email": "star2018@outlook.com",
    "url": "https://github.com/icesjs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "resources",
    "babel.config.js"
  ],
  "keywords": [
    "build",
    "electron",
    "react",
    "webpack",
    "app"
  ],
  "license": "MIT"
}
