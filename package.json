    "bindings": "^1.5.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.1",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-notarize": "^1.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "find-up": "^5.0.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.6",
    "js-yaml": "^3.14.1",
    "lint-staged": "^10.5.3",
    "loader-utils": "^2.0.0",
    "minimist": "^1.2.5",
    "portfinder": "^1.0.28",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.1",
    "sass": "^1.30.0",
    "schema-utils": "^3.0.0",
    "strip-ansi": "^6.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "term-size": "^2.2.1",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1",
    "webpack-merge": "^5.7.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "build": {
    "extends": null,
    "extraMetadata": {
      "main": "index.js"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-css-modules"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,yml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "url": "https://github.com/icesjs/viper"
  },
  "homepage": "."
}
