{
  "name": "viper",
  "version": "0.1.0",
  "private": true,
  "description": "An video player for free.",
  "author": {
    "name": "icesjs",
    "email": "icesjs@outlook.com",
    "url": "https://github.com/icesjs"
  },
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start",
    "build": "node scripts/build",
    "test": "craco test",
    "pack": "yarn build && electron-builder -c build.yml --dir",
    "dist": "yarn build && electron-builder -c build.yml -mlw",
    "postinstall": "electron-builder install-app-deps && yarn-deduplicate yarn.lock"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "electron-log": "^4.3.0",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "fs-extra": "^9.0.1",
    "node-fetch": "^2.6.1",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.0.0",
    "@reduxjs/toolkit": "^1.2.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "babel-plugin-react-css-modules": "^5.2.6",
    "chokidar": "^3.4.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-notarize": "^1.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "find-up": "^5.0.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "loader-utils": "^2.0.0",
    "portfinder": "^1.0.28",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "respawn": "^2.6.0",
    "sass": "^1.30.0",
    "schema-utils": "^3.0.0",
    "strip-ansi": "^6.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "tree-kill": "^1.2.2",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.0",
    "wait-on": "^5.2.0",
    "webpack-cli": "~4.1.0",
    "webpack-merge": "^5.7.2",
    "yarn-deduplicate": "^3.1.0"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-css-modules"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,yml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "url": "https://github.com/icesjs/viper"
  },
  "homepage": "."
}
